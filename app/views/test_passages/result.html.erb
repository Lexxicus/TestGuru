<div>
  <h1><%= t('.header', title: @test_passage.test.title) %></h1>
    <% if @test_passage.success? %>
      <h2><%= t('.success', percent: content_tag(:span, @test_passage.percent_of_correct_answers, id="pass_test")).html_safe %></h2>
    <% else %>
      <h2><%= t('.defeat', percent: content_tag(:span, @test_passage.percent_of_correct_answers, id="failed_test")).html_safe %></h2>
    <% end %>
</div>
<%= link_to t('helpers.links.back'), tests_path %>
