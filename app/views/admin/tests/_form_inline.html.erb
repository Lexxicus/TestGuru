<%= form_with model: [:admin, test],
              url: update_inline_admin_test_path(test),
              local: true,
              class: 'form-inline',
              html: { style: 'display: none' },
              data: { test_id: test.id } do |form| %>
  <div>
    <p>
      <%= form.text_field :title, class: 'form-control' %>
    </p>
  </div>

    <div>
      <p><%= form.submit t('.submit'), class: 'btn btn-light' %></p>
    </div>
  </div>
<% end %>
